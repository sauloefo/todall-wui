{
  "name": "todall-wui",
  "version": "0.2.0",
  "description": "Web User Interface (WUI) for TodAll.",
  "main": "index.js",
  "scripts": {
    "local": "npx parcel site/index.html --out-dir dist/public",
    "build": "npx tsc --project server/tsconfig.json && npx parcel build site/index.html --out-dir dist/public",
